<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" id="report">
  <xs:element name="report">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="rentity_id">
          <xs:simpleType>
            <xs:restriction base="xs:int">
              <xs:minInclusive value="1" />
              <xs:pattern value="[0-9]{1,3}">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> Only digits[0-9] are accepted. Range is 1-999 </xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element minOccurs="0" name="rentity_branch">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="submission_code" type="submission_type" />
        <xs:element name="report_code" type="report_type" />
        <xs:element minOccurs="0" name="entity_reference" nillable="true">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element minOccurs="0" name="fiu_ref_number" nillable="true">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="submission_date" type="sql_date" />
        <xs:element name="currency_code_local" type="currency_type" />
        <xs:element minOccurs="0" name="reporting_person" type="t_person_registration_in_report" />
        <xs:element minOccurs="0" name="location" type="t_address" />
        <xs:element minOccurs="0" name="reason">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="4000" />
              <xs:pattern value="[ -~\s]*">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> Only ASCII characters are accepted.</xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element minOccurs="0" name="action">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="4000" />
              <xs:pattern value="[ -~\s]*">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> Only ASCII characters are accepted.</xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:choice>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="transaction">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="transactionnumber">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50" />
                        <xs:minLength value="1" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="internal_ref_number">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="50" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="transaction_location">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="0" />
                        <xs:maxLength value="255" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="transaction_description">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="4000" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="date_transaction" type="sql_date" />
                  <xs:element minOccurs="0" maxOccurs="0" name="teller" nillable="true">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" maxOccurs="0" name="authorized" nillable="true">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:sequence minOccurs="0" maxOccurs="0">
                    <xs:element minOccurs="0" name="late_deposit">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:boolean" />
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="date_posting" type="sql_date" />
                  </xs:sequence>
                  <xs:element minOccurs="0" name="value_date" type="sql_date" />
                  <xs:element name="transmode_code" type="conduction_type" />
                  <xs:element minOccurs="0" name="transmode_comment" nillable="true">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="amount_local">
                    <xs:simpleType>
                      <xs:restriction base="xs:decimal">
                        <xs:minInclusive value="0" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:choice>
                    <xs:element name="involved_parties">
                      <xs:complexType>
                        <xs:sequence maxOccurs="unbounded">
                          <xs:element name="party" type="t_party" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:sequence>
                      <xs:choice>
                        <xs:element name="t_from_my_client">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="from_funds_code" type="funds_type" />
                              <xs:element minOccurs="0" name="from_funds_comment">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:maxLength value="255" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element minOccurs="0" name="from_foreign_currency" type="t_foreign_currency" />
                              <xs:element minOccurs="0" name="t_conductor" type="t_person_my_client" />
                              <xs:choice>
                                <xs:element name="from_account" type="t_account_my_client" />
                                <xs:element name="from_person" type="t_person_my_client" />
                                <xs:element name="from_entity" type="t_entity_my_client" />
                              </xs:choice>
                              <xs:element name="from_country" type="country_type" />
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="t_from">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="from_funds_code" type="funds_type" />
                              <xs:element minOccurs="0" name="from_funds_comment">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:maxLength value="255" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element minOccurs="0" name="from_foreign_currency" type="t_foreign_currency" />
                              <xs:element minOccurs="0" name="t_conductor" type="t_person" />
                              <xs:choice>
                                <xs:element name="from_account" type="t_account" />
                                <xs:element name="from_person" type="t_person" />
                                <xs:element name="from_entity" type="t_entity" />
                              </xs:choice>
                              <xs:element name="from_country" type="country_type" />
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:choice>
                      <xs:choice>
                        <xs:element name="t_to_my_client">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="to_funds_code" type="funds_type" />
                              <xs:element minOccurs="0" name="to_funds_comment">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:maxLength value="255" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element minOccurs="0" name="to_foreign_currency" type="t_foreign_currency" />
                              <xs:choice>
                                <xs:element name="to_account" type="t_account_my_client" />
                                <xs:element name="to_person" type="t_person_my_client" />
                                <xs:element name="to_entity" type="t_entity_my_client" />
                              </xs:choice>
                              <xs:element name="to_country" type="country_type" />
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="t_to">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="to_funds_code" type="funds_type" />
                              <xs:element minOccurs="0" name="to_funds_comment">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:maxLength value="255" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element minOccurs="0" name="to_foreign_currency" type="t_foreign_currency" />
                              <xs:choice>
                                <xs:element name="to_account" type="t_account" />
                                <xs:element name="to_person" type="t_person" />
                                <xs:element name="to_entity" type="t_entity" />
                              </xs:choice>
                              <xs:element name="to_country" type="country_type" />
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:choice>
                    </xs:sequence>
                  </xs:choice>
                  <xs:element minOccurs="0" name="goods_services">
                    <xs:complexType>
                      <xs:sequence maxOccurs="unbounded">
                        <xs:element name="item" type="t_trans_item" />
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="comments">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="0" />
                        <xs:maxLength value="4000" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:element name="activity">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="report_parties">
                  <xs:complexType>
                    <xs:sequence maxOccurs="unbounded">
                      <xs:element name="report_party" type="report_party_type" />
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="goods_services">
                  <xs:complexType>
                    <xs:sequence maxOccurs="unbounded">
                      <xs:element name="item" type="t_trans_item" />
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
        <xs:element name="report_indicators">
          <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
              <xs:element name="indicator" type="report_indicator_type" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="t_account_my_client">
    <xs:sequence>
      <xs:element name="institution_name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="3" />
            <xs:maxLength value="255" />
            <xs:pattern value="[ \t\n&amp;'\\-`,\\/:.;,_~A-Za-z0-9()-]+">
              <xs:annotation>
                <xs:documentation xml:lang="en"> accepted characters are (space/tab/new line), A-Z, a-z, 0-9, ampersand,(apostrophe), ., -,(, ), `, \, /, :, ;, ,(comma), _, ~, @, $. Must have characters are A-Z, a-z (letters)</xs:documentation>
              </xs:annotation>
            </xs:pattern>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element minOccurs="0" maxOccurs="0" name="institution_code" nillable="true">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="50" />
              <xs:pattern value="[0-9]+">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> Accepted values are digits and minimum value is 1 </xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="swift">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="11" />
              <xs:minLength value="4" />
              <xs:pattern value="[A-Z]{4}LK[A-Z0-9]{2}([A-Z0-9]{3})?|[0-9]{4}">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> SWIFT code format and at minimum 8 chars and maximum 11 OR LankaClear code (4 digits) OR LankaFIN code (4 digits, add prededing zeros to match 4 digits if LankaFIN code is just 2 digits)</xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" name="non_bank_institution" type="xs:boolean" />
      <xs:element name="branch">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
            <xs:minLength value="3" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="account">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
            <xs:minLength value="3" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="currency_code" type="currency_type" />
      <xs:element minOccurs="0" name="account_name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="iban">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="34" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="client_number">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="30" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="personal_account_type" type="account_type" />
      <xs:element minOccurs="0" name="t_entity" type="t_entity_my_client" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="signatory">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" fixed="1" name="is_primary">
              <xs:simpleType>
                <xs:restriction base="xs:boolean" />
              </xs:simpleType>
            </xs:element>
            <xs:element name="t_person" type="t_person_my_client" />
            <xs:element name="role" type="account_person_role_type" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="opened" type="sql_date" />
      <xs:element minOccurs="0" name="closed" type="sql_date" />
      <xs:sequence minOccurs="0">
        <xs:element minOccurs="0" name="balance" type="xs:decimal" />
        <xs:element minOccurs="0" name="date_balance" type="sql_date" />
      </xs:sequence>
      <xs:element name="status_code" type="account_status_type" />
      <xs:element minOccurs="0" name="beneficiary" nillable="true">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="beneficiary_comment" nillable="true">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="comments">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="4000" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="t_account">
    <xs:sequence>
      <xs:element name="institution_name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="3" />
            <xs:maxLength value="255" />
            <xs:pattern value="[ \t\n&amp;'\\-`,\\/:.;,_~A-Za-z0-9()-]+">
              <xs:annotation>
                <xs:documentation xml:lang="en"> accepted characters are (space/tab/new line), A-Z, a-z, 0-9, ampersand,(apostrophe), ., -,(, ), `, \, /, :, ;, ,(comma), _, ~, @, $. Must have characters are A-Z, a-z (letters)</xs:documentation>
              </xs:annotation>
            </xs:pattern>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element minOccurs="0" maxOccurs="0" name="institution_code" nillable="true">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="50" />
              <xs:pattern value="[0-9]+">
                <xs:annotation>
                  <xs:documentation xml:lang="en"> Accepted values are digits and minimum value is 1 </xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="swift">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="11" />
              <xs:minLength value="4" />
              <xs:pattern value="[A-Z]{6}[A-Z0-9]{2}([A-Z0-9]{3})?|[0-9]{4}">
                <xs:annotation>
                  <xs:documentation xml:lang="en">SWIFT code format and at minimum 8 chars and maximum 11 OR LankaClear code (4 digits) OR LankaFIN code (4 digits, add prededing zeros to match 4 digits if LankaFIN code is just 2 digits)</xs:documentation>
                </xs:annotation>
              </xs:pattern>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" name="non_bank_institution" type="xs:boolean" />
      <xs:element minOccurs="0" name="branch">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="account">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
            <xs:minLength value="3" />
            <xs:pattern value="[A-Za-z0-9\\|_/\\-]+">
              <xs:annotation>
                <xs:documentation xml:lang="en"> account number may contain any of A-Z(uppercase), a-z(lowercase), 0-9, \, /, |, _, -. Must contain any of A-Z(uppercase), a-z(lowercase), 0-9</xs:documentation>
              </xs:annotation>
            </xs:pattern>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="currency_code" type="currency_type" />
      <xs:element minOccurs="0" name="account_name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="iban">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="34" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="client_number">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="30" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="personal_account_type" type="account_type" />
      <xs:element minOccurs="0" name="t_entity" type="t_entity" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="signatory">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" fixed="1" name="is_primary">
              <xs:simpleType>
                <xs:restriction base="xs:boolean" />
              </xs:simpleType>
            </xs:element>
            <xs:element name="t_person" type="t_person" />
            <xs:element minOccurs="0" name="role" type="account_person_role_type" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="opened" type="sql_date" />
      <xs:element minOccurs="0" name="closed" type="sql_date" />
      <xs:sequence minOccurs="0">
        <xs:element minOccurs="0" name="balance" type="xs:decimal" />
        <xs:element minOccurs="0" name="date_balance" type="sql_date" />
      </xs:sequence>
      <xs:element minOccurs="0" name="status_code" type="account_status_type" />
      <xs:element minOccurs="0" name="beneficiary" nillable="true">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="beneficiary_comment" nillable="true">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="comments">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="4000" />
            <xs:pattern value="[ -~\s]*">
              <xs:annotation>
                <xs:documentation xml:lang="en"> only ASCII characters are allowed</xs:documentation>
              </xs:annotation>
            </xs:pattern>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="t_entity_my_client">
    <xs:sequence>
      <xs:element name="name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
            <xs:minLength value="2" />
            <xs:pattern value="([\sA-Za-z0-9&amp;'\.\-\(\)`\\/:;,_~@]{2,})(.*[A-Za-z]+).*">
              <xs:annotation>
                <xs:documentation xml:lang="en">accepted characters are (space/tab/new line), A-Z(uppercase), a-z(lowercase), 0-9, ampersand,(apostrophe), ., -,(, ), `, \, /, :, ;, ,(comma), _, ~, @, $. Must have characters are A-Z, a-z (letters)</xs:documentation>
              </xs:annotation>
            </xs:pattern>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="commercial_name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="incorporation_legal_form" type="legal_form_type" />
      <xs:element minOccurs="0" name="incorporation_number">
        <xs:simpleType>
          <xs:restriction base="xs:string">
